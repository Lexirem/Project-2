<section>
  
  <a href="/oneTheme/blackandwhite" class='#'>
    <h2>Black and White</h2>
  </a>
  <br>
  {{#each photosblackandwhite }}
  <div class="photo-block">
    <a href="/onePhoto/{{this._id}}">
      <img class="image" src="{{this.image}}" alt="{{this.title}}">
    </a>
  </div>
  {{/each}}
<br>

