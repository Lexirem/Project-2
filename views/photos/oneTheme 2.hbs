<h3>{{photos.0.theme}}</h3> 

{{#each photos}}
<h2>{{this.title}}</h2>
<a class="image" href="/onePhoto/{{_id}}">
    <img class="image"
        src="{{this.image}}"
        alt="{{this.title}}">
</a>
<p>Description: {{this.description}}</p>
<h4>Price: {{this.price}}</h4>
<form action="/add-to-cart/{{this._id}}" method="POST"><button class="addToCart" type="submit">Add to cart</button></form>
{{/each}}