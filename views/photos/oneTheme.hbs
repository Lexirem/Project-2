{{!-- aqui va el titulo del theme y fotos de ese theme --}}

<h3>{{this.theme}}</h3>
{{#each photos}}
<h2>{{this.title}}</h2>
<a class="#" href="/onePhoto/{{_id}}">
    <img class="image"
        src="{{this.image}}"
        alt="{{this.title}}">
</a>
<p>Description: {{this.description}}</p>
<h4>Price: {{this.price}}</h4>
<button class="addToCart">Add to cart</button>
{{/each}}